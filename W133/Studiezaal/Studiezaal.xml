	<hotspot name="logo" keep="true"
		url="../../../img/logo.png"
		ath="0"
		atv="90"
		scale="0.5"
	/>
	
	<hotspot name="hotspot-0" scale="0.8" style="hotspot" ath="147" atv="-2" title="RuimteVoor" onclick="js(vr_loadpano(../RuimteVoor/RuimteVoor.xml))" url="../../../img/hotspot.png" />
</krpano>
